import java.util.Scanner;

public class arrays {
   public arrays() {
   }

   public static void main(String[] var0) {
      int[] var1 = new int[100];
      Scanner var2 = new Scanner(System.in);
      var1[0] = var2.nextInt();
      var1[1] = var2.nextInt();
      var1[2] = var2.nextInt();
      System.out.println("phy: " + var1[0]);
      System.out.println("chem: " + var1[1]);
      System.out.println("math: " + var1[2]);
      var2.close();
   }
}
